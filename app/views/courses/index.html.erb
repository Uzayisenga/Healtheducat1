<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Credit number</th>
      <th>Content</th>
      <th>Upload file</th>
      <th>Course price</th>
      <th>Status</th>
      <th>Marks</th>
      <th>Attempty</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.image %></td>
        <td><%= course.title %></td>
        <td><%= course.credit_number %></td>
        <td><%= course.content %></td>
        <td><%= course.upload_file %></td>
        <td><%= course.course_price %></td>
        <td><%= course.status %></td>
        <td><%= course.marks %></td>
        <td><%= course.attempty %></td>
        <td><%= course.user_id %></td>
        <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
